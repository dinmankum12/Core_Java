CREATE TABLE Passenger_Details (
    Passenger_id INT PRIMARY KEY,
    Passenger_name VARCHAR(255),
    Passenger_dob DATE,
    Passenger_phone VARCHAR(15),
    Passenger_email VARCHAR(255)
);


